<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dev.spring.petclinic.old.searchResult.mapper.OwnersMapper">
    <select id="selectOwnerById" parameterType="int" resultType="dev.spring.petclinic.old.searchResult.model.Owners">
        SELECT * FROM owners WHERE id = #{id}
